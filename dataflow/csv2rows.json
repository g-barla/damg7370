{
	"name": "csv2rows",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_nameBasics",
						"type": "DatasetReference"
					},
					"name": "dsnameBasics"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     parameter1 as string",
				"}",
				"source(output(",
				"          {nconst     primaryName     birthYear     deathYear     primaryProfession     knownForTitles} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> dsnameBasics",
				"dsnameBasics derive({} = ERROR_FUNCTION('')) ~> derivedColumn1"
			]
		}
	}
}