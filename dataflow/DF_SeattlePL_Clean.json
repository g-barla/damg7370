{
	"name": "DF_SeattlePL_Clean",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText_Dataset",
						"type": "DatasetReference"
					},
					"name": "readTSVFile"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {License Issue Date} as string,",
				"          {License Number} as string,",
				"          {Animal's Name} as string,",
				"          Species as string,",
				"          {Primary Breed} as string,",
				"          {Secondary Breed} as string,",
				"          {ZIP Code} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false) ~> readTSVFile",
				"readTSVFile derive(column1 = ERROR_FUNCTION(''),",
				"          column2 = ERROR_FUNCTION('')) ~> derivedColumn1"
			]
		}
	}
}